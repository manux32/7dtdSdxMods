<configs>
	<config name="vehicles">
		<append xpath="/vehicles/vehicle[@name='minibike']">
			<!-- MANUX Custom Vehicles: Empty slots are there to show nothing in extra slots for vehicles that have less than 10 slots, or for the default minibike which does not support weapon parts -->
			<property class="empty1">
				<property name="class" value="Empty, Mods" />
				<property name="slot_type" value="empty" />
				<property name="parent_part" value="chassis" />
			</property>
			<property class="empty2">
				<property name="class" value="Empty, Mods" />
				<property name="slot_type" value="empty" />
				<property name="parent_part" value="chassis" />
			</property>
		</append>
	</config>

	<config name="loot">
		<append xpath="/lootcontainers">
			<!-- MANUX Custom Vehicles: different size storage -->
			<lootcontainer id="189" count="0" size="4,6" sound_open="UseActions/open_shopping_basket" open_time="0" sound_close="UseActions/close_shopping_basket" loot_quality_template="baseTemplate"></lootcontainer>
			<lootcontainer id="190" count="0" size="5,9" sound_open="UseActions/open_shopping_basket" open_time="0" sound_close="UseActions/close_shopping_basket" loot_quality_template="baseTemplate"></lootcontainer>
			<lootcontainer id="191" count="0" size="8,10" sound_open="UseActions/open_shopping_basket" open_time="0" sound_close="UseActions/close_shopping_basket" loot_quality_template="baseTemplate"></lootcontainer>
		</append>
	</config>
	
	<config name="buffs">
		<set xpath="/buffs/buff[@id='cannotBreath']/@mutex">drowning,airtight</set>
		<remove xpath="/buffs/buff[@id='drowning']"/>
		<remove xpath="/buffs/buff[@id='wet']"/>
		<insertAfter xpath="/buffs/buff[@id='cannotBreath']">
			<buff id="stopAirtight" duration="1" stack="discard" actions="debuff(airtight);debuff(airtightWarming);debuff(airtightCooling)" name_key="stopAirtight"/>
			<buff id="airtightWarming" duration="0" stack="discard" mutex="stopAirtight" requires="airtight" buffif="coretemp less 65" debuffif="coretemp greater 70" name_key="airtightWarming" description_key="airtightWarmingDesc" icon="ui_game_symbol_coffee">
				<modify id="0" stat="coretemp" amount="1" rate="1" />
			</buff>
			<buff id="airtightCooling" duration="0" stack="discard" mutex="stopAirtight" requires="airtight" buffif="coretemp greater 75" debuffif="coretemp less 70" name_key="airtightCooling" description_key="airtightCoolingDesc" icon="ui_game_symbol_cold">
				<modify id="0" stat="coretemp" amount="-1" rate="1" />
			</buff>
			<buff id="airtight" duration="0" stack="discard" mutex="stopAirtight" actions="debuff(cannotBreath);debuff(cannotBreatheWrn);debuff(drowning)" name_key="airtight" description_key="airtightDesc" tooltip_key="airtightDesc" icon="manux_ui_game_symbol_oxygen_source"/>			
			<buff id="drowning" duration="0" stack="discard" mutex="airtight" actions="damage(0,4,0,0);debuff(burningSmall);debuff(burning);debuff(burningMolotov)" type="drowning" icon="ui_game_symbol_oxygen" name_key="drowning" description_key="drowningDesc" tooltip_key="drowningTooltip">
				<modify id="1" stat="health" amount="-14" rate="4" />
			</buff>
			<buff id="wet" notificationClass="WetBuffNotification" duration="0" stack="discard" mutex="airtight" buffif="waterLevel greater 0" debuffif="waterLevel equals 0" icon="ui_game_symbol_wet" name_key="wet" description_key="wetDesc" tooltip_key="wetTooltip" />
		</insertAfter>
	</config>
	
